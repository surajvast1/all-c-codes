# include<stdio.h>
int main()
{    
   int a[5]={1,2,3,4,5};
   int d=0,b = 4,mid,c;

   printf("enter number to find\n");
   scanf("%d",&c);
   
   while(d<=b)
   {
     mid = (d+b)/2;
   if (c==a[mid])
   {
       printf("no found");
       break;
   }
   else if (c>a[mid])
    {
        d=mid+1;

     }
    else{
        d=mid-1;
    }
   }
   if (d>b)
   {
   printf("no not found");
   }
   return 0;


}
   
